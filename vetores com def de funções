#include <stdio.h>

void imprime(int v[5]){
	int i;
	for(i=0;i<5;i++){
		printf("%d", v[i]);
	}
}

//ou

void imprime2(int v[], int n){
	int i;
	for(i=0;i<n;i++){
		printf("%d", v[i]);
	}
}


int main(){
	
	int vet[5] = {1, 2, 3, 4, 5};
	
	puts("Imprimindo: \n");
	imprime(vet);
	
	puts("\n");
	
	puts("Imprimindo: \n");
	imprime2(vet, 5);
}
